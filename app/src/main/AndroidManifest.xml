<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/talentme_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TalentMe"
        tools:targetApi="31">
        <activity
            android:name=".ProfilePageActivity"
            android:exported="false" />
        <activity
            android:name=".ui.result.personality.ResultPersonalityActivity"
            android:exported="false" />
        <activity
            android:name=".ui.result.passion.ResultPassionActivity"
            android:exported="false" />
        <activity
            android:name=".ui.result.talent.ResultTalentActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".ui.startpage.StartPageActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".ui.profile.ProfileActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.test.personality.TestPersonalityActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".ui.test.passion.TestPassionActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".ui.test.talent.TestTalentActivity"
            android:exported="false">
        </activity>
    </application>

</manifest>